{
  "name": "acmekit-starter-default",
  "version": "0.0.1",
  "description": "A starter for Acmekit projects.",
  "author": "Acmekit (https://acmekit.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "acmekit"
  ],
  "scripts": {
    "build": "acmekit build",
    "seed": "acmekit exec ./src/scripts/seed.ts",
    "start": "acmekit start",
    "dev": "acmekit develop",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@acmekit/acmekit": "2.13.1",
    "@acmekit/admin-sdk": "2.13.1",
    "@acmekit/cli": "2.13.1",
    "@acmekit/framework": "2.13.1"
  },
  "devDependencies": {
    "@acmekit/test-utils": "2.13.1",
    "@swc/core": "^1.7.28",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.14",
    "yalc": "^1.0.0-pre.53"
  },
  "engines": {
    "node": ">=20"
  },
  "bin": {
    "acmekit": "node_modules/@acmekit/cli/cli.js"
  }
}
